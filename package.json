    "moment": "^2.29.1",
    "@typescript-eslint/parser": "^5.50.0",
    "vue": "^2.6.14",
    "vue-authenticate": "github:alerta/vue-authenticate",
    "vue-class-component": "^7.2.6",
    "vue-i18n": "^8.28.2",
    "vue-object-merge": "^1.0.0",
    "vue-property-decorator": "^9.1.2",
